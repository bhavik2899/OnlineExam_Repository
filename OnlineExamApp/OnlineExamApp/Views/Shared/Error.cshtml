<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Error</title>
</head>
<body>
	<hgroup>
		<h1>Error.</h1>
		@{
			string error = "";
			try { error = TempData["errMsg"].ToString(); } catch { }
		}
		<h2>An error occurred while processing your request.</h2>
		<h2>@error</h2>
	</hgroup>
</body>
</html>
